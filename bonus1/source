//       ebp+0x8 | ebp+0xc
int main(int ac, char ** av) {
	int res;  // esp+0x3c
	res = atoi(av[1]);
	if (res > 9) {
		return 1;
	}
	// *main+43

	char str[40];  // esp+0x14
	memcpy(str, av[2], res*4);
	if (res == 1464814662) {  // 0x574f4c46
		execl("/bin/sh", "sh", 0);
	}
	// *main+122
	return 0;
}